{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-tabs": 0,
        "indent": 0,
        "space-before-function-paren": 0,
        "no-new": 0,
        "no-trailing-spaces": 0,
        "spaced-comment": 0,
        "no-multiple-empty-lines": 0,
        "no-unused-vars": 0,
        "eol-last": 0,
        "prefer-const": 0,
        "no-undef": 0
    }
}
